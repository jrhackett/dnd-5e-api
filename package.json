{
  "name": "dnd-5e-api",
  "version": "0.0.1",
  "description": "An API to fetch information about DnD 5e spells.",
  "main": "server.js",
  "scripts": {
    "coverage": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --compilers js:babel-core/register ./test/*.test.js",
    "test": "NODE_ENV=test nyc ./node_modules/.bin/mocha --compilers js:babel-core/register ./test/*.test.js",
    "start": "nodemon server/server.js --exec babel-node",
    "build": "babel server -d build",
    "serve": "node build/server.js"
  },
  "author": "Jacob Hackett",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "jsonfile": "^3.0.0",
    "mongoose": "^4.10.2",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "tap": "^10.5.2"
  }
}
